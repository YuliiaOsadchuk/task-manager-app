{
  "name": "task-manager-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/icons-material": "5.2.0",
    "@mui/lab": "5.0.0-alpha.57",
    "@mui/material": "5.2.2",
    "@reduxjs/toolkit": "1.6.2",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.20",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "5.3.2",
    "@types/redux-logger": "3.0.9",
    "@types/styled-components": "5.1.16",
    "axios": "0.24.0",
    "concurrently": "6.4.0",
    "formik": "2.2.9",
    "formik-mui": "4.0.0-alpha.3",
    "formik-mui-lab": "1.0.0-alpha.3",
    "json-server": "0.17.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.0.2",
    "react-scripts": "4.0.3",
    "react-toastify": "8.1.0",
    "redux-logger": "3.0.6",
    "styled-components": "5.3.3",
    "typescript": "4.1.2",
    "web-vitals": "1.0.1",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "concurrently \"npm run start:ui\" \"npm run start:server\"",
    "start:ui": "react-scripts start",
    "start:server": "json-server -w server/db.json -p 3001",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "prettier": "prettier --check \"src/**/*.{tsx, ts}\"",
    "prettier:fix": "prettier --write \"src/**/*.{tsx, ts}\"",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "lint:staged": "lint-staged --relative",
    "stylelint": "stylelint \"./src/**/*.styles.tsx\"",
    "pre-commit-check": "npm run lint:staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run prettier:fix",
      "npm run stylelint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.0",
    "lint-staged": "12.1.2",
    "prettier": "2.5.0",
    "pretty-quick": "3.1.2",
    "stylelint": "14.1.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
